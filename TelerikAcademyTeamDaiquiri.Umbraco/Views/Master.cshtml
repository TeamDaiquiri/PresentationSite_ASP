@inherits UmbracoTemplatePage
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="assets/paper_img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@CurrentPage.Site().Name</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link href="/Content/bootstrap3/css/bootstrap.css" rel="stylesheet" />
    <link href="/Content/Assets/css/ct-paper.css" rel="stylesheet" />
    <link href="/Content/Assets/css/demo.css" rel="stylesheet" />

    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300' rel='stylesheet' type='text/css'>
</head>
<body>
    @Html.Partial("HeaderNavigation")
    @RenderBody()

    <script src="/Content/Assets/js/jquery-1.10.2.js" type="text/javascript"></script>
    <script src="/Content/Assets/js/jquery-ui-1.10.4.custom.min.js" type="text/javascript"></script>
    <script src="/Content/bootstrap3/js/bootstrap.js" type="text/javascript"></script>

    <!--  Plugins -->
    <script src="/Content/Assets/js/ct-paper-checkbox.js"></script>
    <script src="/Content/Assets/js/ct-paper-radio.js"></script>
    <script src="/Content/Assets/js/bootstrap-select.js"></script>
    <script src="/Content/Assets/js/bootstrap-datepicker.js"></script>

    <script src="/Content/Assets/js/ct-paper.js"></script>  
